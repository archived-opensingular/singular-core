<ehcache xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
         xsi:noNamespaceSchemaLocation="ehcache.xsd"
         updateCheck="true"
         monitoring="autodetect"
         dynamicConfig="true">

    <diskStore path="java.io.tmpdir"/>

    <defaultCache timeToIdleSeconds="120"
                  timeToLiveSeconds="120"
                  maxBytesLocalHeap="102400"
                  eternal="false"
                  maxEntriesLocalDisk="10000000"
                  diskExpiryThreadIntervalSeconds="120"
                  transactionalMode="off"
                  memoryStoreEvictionPolicy="LRU">
        <persistence strategy="none"/>
    </defaultCache>

    <cache name="defaultCache"
        timeToIdleSeconds="120"
        timeToLiveSeconds="120"
        maxBytesLocalHeap="102400"
        eternal="false"
        maxEntriesLocalDisk="10000000"
        diskExpiryThreadIntervalSeconds="120"
        transactionalMode="off"
        memoryStoreEvictionPolicy="LRU">
        <persistence strategy="none"/>
    </cache>


    <cache name="oneSecond"
           timeToIdleSeconds="1"
           timeToLiveSeconds="1"
           maxBytesLocalHeap="102400"
           eternal="false"
           maxEntriesLocalDisk="10000000"
           transactionalMode="off"
           memoryStoreEvictionPolicy="LRU">
        <persistence strategy="none"/>
    </cache>

    <cache name="forever" eternal="true"
        maxBytesLocalHeap="102400"
        maxEntriesLocalDisk="10000000"
        transactionalMode="off"
        memoryStoreEvictionPolicy="LRU">
        <persistence strategy="none"/>
    </cache>

    <cache name="retrieveGroup" eternal="false"
           maxEntriesLocalHeap="10000"
           timeToIdleSeconds="300"
           timeToLiveSeconds="600"
           transactionalMode="off"
           memoryStoreEvictionPolicy="LFU">
        <persistence strategy="none" synchronousWrites="false"/>
    </cache>

    <cache name="retrieveGroupByProcess" eternal="false"
